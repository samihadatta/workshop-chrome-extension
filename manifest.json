{
    "manifest_version": 2,
    "name": "Custom Google Homepage",
    "description": "This extension does shows a picture upon navigation to Google",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://www.google.com/*"],
            "css": ["main.css"]
        }
    ],
    "browser_action": {
        "default_title": "My personalized google page!",
        "default_popup": "popup.html"
    }
   }